{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@/components/*": ["/components/*"],
      "@/contexts/*": ["/contexts/*"],
      "@/features/*": ["/features/*"],
      "@/hooks/*": ["/hooks/*"],
      "@/intl/*": ["/intl/*"],
      "@/layout/*": ["/layout/*"],
      "@/providers/*": ["/providers/*"],
      "@/services/*": ["/services/*"],
    }
  }
}